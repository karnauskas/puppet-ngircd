# Server <%= @name %>:<%= @port %>
[Server]
  Name = <%= @name %>
  Host = <%= @host %>
  Port = <%= @port %>
  Bind = <%= @bind %>
  MyPassword = <%= @local_password %>
  PeerPassword = <%= @remote_password %>
  Group = <%= @group %>
  Passive = <%= @passive %>
  SSLConnect = <%= @ssl %>
  ServiceMask = <%= @service_mask %>
